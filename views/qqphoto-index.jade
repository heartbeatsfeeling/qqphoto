doctype html
html(lang='zh-CN')
	head
		title QQ头像 QQ男生头像 QQ女生头像 QQ情侣头像 QQ头像网 
		meta(http-equiv="Content-Type",content="text/html; charset=utf-8")
		meta(name="viewport",content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no")
		meta(name="keywords",content="最新的QQ头像 QQ男生头像 QQ女生头像 QQ情侣头像 QQ头像网")
		meta(name="description" content="欢迎来到QQ头像网，本站为您提供最新的QQ头像 QQ男生头像 QQ女生头像 QQ情侣头像等。")
		link(type="text/css",rel="stylesheet",href="/css/style.css")
		link(type="text/css",rel="stylesheet",href="/css/common.css")
	body
		.header-index
			.container.clearfix
				.logo.l
					a(href="/index.html",title="QQ头像网") QQ头像网
				.header-nav.r
					//ul
						li
							a(href="/index.html",title="QQ男生头像") QQ男生头像
						li
							a(href="/index.html",title="QQ女生头像") QQ女生头像
						li
							a(href="/index.html",title="QQ情侣头像") QQ情侣头像
		.container.main
			.item
				include com/qqphoto-sliderNav
			.item.hot
				h2 热门头像
					ul.clearfix
						each item in data
							li(data-id="#{item._id}")
								.inner
									.img
										a(href="/article?id=#{item._id}",target="_blank")
											img(src="upload/#{item.imgSrc[0].name}",width=180,height=180,title="#{item.title}")
									.desc #{item.desc}
									.info.clearfix
										.span.l #{item.updateTime}
										span.r #{item.author}
									.like
										i.icon-xin.icon.up(title="不错")
											span #{item.up}
										i.icon-xinsui.icon.down(title="太差")
											span #{item.down}
										i.icon-liulan.icon(title="浏览")
											span #{item.view}
		.footer-index
			.container
				p ©Copyright 2015 qq头像网
				p 版权声明：本站内容均来自于网络，如需删除请联系网站管理员
		script(src="/js/lib/require.js",type="text/javascript",data-main="/js/config")
		script(src="/js/common.js",type="text/javascript")